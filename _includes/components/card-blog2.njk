{% macro show(title, cta) %}

<div class='card-blog2'>
    <div class='card__img'>
        <img src='https://www.fillmurray.com/300/300' />
    </div>
    <div class='card__desc'>
        <h2 class='desc__title'>{{ title }}</h2>
        
        <a class='cta cta--seemore'>{{ cta }}</a>
    </div>
</div>

{% endmacro %}